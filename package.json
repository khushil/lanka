{
  "name": "lanka",
  "version": "0.1.0",
  "description": "Revolutionary Graph-Based AI Development Environment",
  "main": "dist/index.js",
  "scripts": {
    "setup": "npm install && npm run setup:db",
    "setup:db": "docker-compose up -d neo4j",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:bdd": "cucumber-js",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist coverage",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": ["AI", "graph", "development", "intelligence", "neo4j"],
  "author": "LANKA Development Team",
  "license": "MIT",
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "@neo4j/graphql": "^5.3.0",
    "@tensorflow/tfjs-node": "^4.17.0",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "fastapi": "^0.1.0",
    "graphql": "^16.8.1",
    "ioredis": "^5.3.2",
    "kafkajs": "^2.2.4",
    "langchain": "^0.1.25",
    "mongodb": "^6.3.0",
    "neo4j-driver": "^5.17.0",
    "openai": "^4.28.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^10.3.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}