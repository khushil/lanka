    "@reactflow/node-resizer": "^2.2.14",
    "@reactflow/node-toolbar": "^1.3.14",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/d3-force": "^3.0.10",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/three": "^0.179.0",
    "axios": "^1.11.0",
    "d3-force": "^3.0.0",
    "date-fns": "^4.1.0",
    "formik": "^2.4.6",
    "graphql": "^16.11.0",
    "html2canvas": "^1.4.1",
    "prismjs": "^1.30.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-force-graph": "^1.48.0",
    "react-force-graph-2d": "^1.28.0",
    "react-force-graph-3d": "^1.28.0",
    "react-router-dom": "^7.8.0",
    "react-scripts": "5.0.1",
    "reactflow": "^11.11.4",
    "recharts": "^3.1.2",
    "socket.io-client": "^4.8.1",
    "three": "^0.179.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4",
    "yup": "^1.7.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:production": "REACT_APP_ENV=production react-scripts build",
    "build:staging": "REACT_APP_ENV=staging react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:ci": "CI=true react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "analyze": "npm run build && npx bundle-analyzer build/static/js/*.js",
    "docker:build": "docker build -t lanka-ui:latest .",
    "docker:run": "docker run -p 3000:3000 lanka-ui:latest",
    "docker:dev": "docker-compose up --build",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up --build",
    "deploy:staging": "npm run build:staging && npm run docker:build",
    "deploy:production": "npm run build:production && npm run docker:build",
    "precommit": "npm run lint && npm run typecheck && npm run test:ci",
    "serve": "serve -s build -l 3000"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
